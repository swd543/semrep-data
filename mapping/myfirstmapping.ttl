@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix sc: <http://schema.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix umls: <http://www.w3id.org/umls/>.
@base <http://www.w3id.org/umls>.

<TriplesMapCuiA>
    a rr:TriplesMap;
    rml:logicalSource [
        rml:source "mapping/input/input.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://www.w3id.org/umls/CUI_A/{2}";
        rr:class umls:CUI_A;
    ];
    rr:predicateObjectMap [ 
        rr:predicate rdfs:label ;
        rr:objectMap [ rml:reference "3" ]
    ] ;
    rr:predicateObjectMap [
        rr:predicateMap [ rr:template "http://www.w3id.org/umls/{8}";];
        rr:objectMap [ rr:template "http://www.w3id.org/umls/CUI_B/{9}" ];
    ].

<TriplesMapCuiB>
    a rr:TriplesMap;
    # Define the data source (file location)
    rml:logicalSource [
        rml:source "mapping/input/input.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://www.w3id.org/umls/CUI_B/{9}";
        rr:class umls:CUI_B;
    ];
    rr:predicateObjectMap [ 
        rr:predicate rdfs:label ;
        rr:objectMap [ rml:reference "10" ]
    ].

<TriplesMapActiveDrugIngredient>
    a rr:TriplesMap;
    # Define the data source (file location)
    rml:logicalSource [
        rml:source "XMLProduct_DBID_2/XMLProduct_DBID_2.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://www.w3id.org/umls/Active_ingredient/{DB_ID}";
        rr:class umls:Active_ingredient;
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [ rml:reference "Drug_name" ];
    ].

<TriplesMapDrugs>
    a rr:TriplesMap;
    # Define the data source (file location)
    rml:logicalSource [
        rml:source "XMLProduct_DBID_2/XMLProduct_DBID_2.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://www.w3id.org/umls/Drug/{Set_ID}";
        rr:class umls:Drug;
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [ rml:reference "Drug_Brand_Name" ];
    ];
    rr:predicateObjectMap [
        rr:predicate umls:Active_ingredient;
        rr:objectMap [ rr:template "http://www.w3id.org/umls/Active_ingredient/{DB_ID}" ];
    ].

